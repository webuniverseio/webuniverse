<!doctype html>
<html lang="<%= config.language %>" class="no-js">
<head prefix="og: http://ogp.me/ns# fb: http://ogp.me/ns/fb# article: http://ogp.me/ns/article#">
	<meta http-equiv="x-dns-prefetch-control" content="on">
	<link rel="dns-prefetch" href="//webuniverse.disqus.com">
	<link rel="dns-prefetch" href="//a.disquscdn.com">
	<link rel="dns-prefetch" href="//disqus.com">
	<link rel="dns-prefetch" href="//glitter-services.disqus.com">
	<link rel="dns-prefetch" href="//referrer.disqus.com">

	<meta name="twitter:card" content="summary" />
	<meta name="twitter:site" content="@webuniverseio" />
	<meta property="og:url" content="<%= url %>" />
	<meta name="twitter:url" content="<%= url %>" />
	<%
		/*global page, theme*/
		var title = config.title;
		if (page.title) {
			title = [page.title, title].join(' | ')
		}
	%>
	<meta property="og:title" content="<%= title %>" />
	<meta name="twitter:title" content="<%= title %>" />
	<meta property="og:site_name" content="<%= config.title %>"/>
	<meta property="og:type" content="<%= is_post() ? 'article' : 'website' %>" />
	<% if (page.layout === 'post') { %>
	<meta property="article:publisher" content="https://www.facebook.com/webuniverseio"/>
	<meta property="article:published_time" content="<%= page.date.format() %>"/>
	<% }
		var description = page.overview ? strip_html(page.overview) : config.description;
    %>
	<meta name="description" content="<%= description %>" />
	<meta property="og:description" content="<%= description %>"/>
	<meta name="twitter:description" content="<%= description %>" />
	<%
		var image = page.image ? page.permalink.replace(/index\.html$/, '') + page.image : fullUrl('favicon-192x192.png');
	%>
	<meta property="og:image" content="<%= image %>"/>
	<meta name="twitter:image" content="<%= image %>" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<meta name="format-detection" content="telephone=no" />
	<meta charset="utf-8" />
	<!--[if lte IE 8]>
	<meta http-equiv="refresh" content="0;URL='https://browser-update.org/update.html'" />
	<![endif]-->
	<title><%= title %></title>
	<script>
		document.documentElement.className = document.documentElement.className.replace('no-js', '');
		document.createElement('picture');
	</script>
	<link rel="canonical" href="<%= page.canonical_url || url %>" />
	<link rel="apple-touch-icon" sizes="57x57" href="<%= url_for("apple-touch-icon-57x57.png") %>">
	<link rel="apple-touch-icon" sizes="114x114" href="<%= url_for("apple-touch-icon-114x114.png") %>">
	<link rel="apple-touch-icon" sizes="72x72" href="<%= url_for("apple-touch-icon-72x72.png") %>">
	<link rel="apple-touch-icon" sizes="144x144" href="<%= url_for("apple-touch-icon-144x144.png") %>">
	<link rel="apple-touch-icon" sizes="60x60" href="<%= url_for("apple-touch-icon-60x60.png") %>">
	<link rel="apple-touch-icon" sizes="120x120" href="<%= url_for("apple-touch-icon-120x120.png") %>">
	<link rel="apple-touch-icon" sizes="76x76" href="<%= url_for("apple-touch-icon-76x76.png") %>">
	<link rel="apple-touch-icon" sizes="152x152" href="<%= url_for("apple-touch-icon-152x152.png") %>">
	<link rel="apple-touch-icon" sizes="180x180" href="<%= url_for("apple-touch-icon-180x180.png") %>">
	<link rel="icon" type="image/png" href="<%= url_for("favicon-192x192.png") %>" sizes="192x192">
	<link rel="icon" type="image/png" href="<%= url_for("favicon-160x160.png") %>" sizes="160x160">
	<link rel="icon" type="image/png" href="<%= url_for("favicon-96x96.png") %>" sizes="96x96">
	<link rel="icon" type="image/png" href="<%= url_for("favicon-16x16.png") %>" sizes="16x16">
	<link rel="icon" type="image/png" href="<%= url_for("favicon-32x32.png") %>" sizes="32x32">
	<meta name="msapplication-TileColor" content="#2b5797">
	<meta name="msapplication-TileImage" content="<%= url_for("mstile-144x144.png") %>">

	<% if (theme.rss){ %>
	<link rel="alternative" href="<%- theme.rss %>" title="<%= config.title %>" type="application/atom+xml">
	<% } %>
	<%- css('static/dest/css/main.v1.1.2') %>
</head>