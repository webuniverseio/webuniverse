<div itemscope itemtype="http://schema.org/<%- schema.type %>">
	<article class="content-entry" itemprop="<%- schema.content %>">
		<% if (page.image) { %>
		<img src="<%- page.image %>" alt="<%= page.title %>" />
		<% } %>
		<% if (page.copyright) { %>
		<span class="image-copyright">&copy; <%- page.copyright %></span>
		<% } %>
		<% if (typeof headless === 'undefined') { %>
		<h1 itemprop="name"><%= page.title %></h1>
		<% } %>
		<%- page.content %>
	</article>
</div>