<div itemscope itemtype="http://schema.org/AboutPage">
	<article class="content-entry" itemprop="mainContentOfPage">
		<h1 itemprop="name"><%= page.title %></h1>
		<% if (page.image) { %>
		<img src="<%- fullUrl(page.image) %>" alt="<%= page.title %>" />
		<% } %>
		<%- page.content %>
	</article>
</div>